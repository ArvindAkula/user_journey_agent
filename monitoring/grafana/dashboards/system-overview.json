{
  "dashboard": {
    "id": null,
    "title": "User Journey Analytics - System Overview",
    "tags": ["user-journey-analytics", "production", "overview"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "System Health Status",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"user-journey-analytics-backend\"}",
            "legendFormat": "Backend",
            "refId": "A"
          },
          {
            "expr": "up{job=\"user-app\"}",
            "legendFormat": "User App",
            "refId": "B"
          },
          {
            "expr": "up{job=\"analytics-dashboard\"}",
            "legendFormat": "Analytics Dashboard",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "green", "value": 1}
              ]
            },
            "mappings": [
              {"options": {"0": {"text": "DOWN"}}, "type": "value"},
              {"options": {"1": {"text": "UP"}}, "type": "value"}
            ]
          }
        },
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Request Rate (req/sec)",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(http_requests_total[5m])) by (service)",
            "legendFormat": "{{service}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {"label": "Requests/sec", "min": 0},
          {"show": false}
        ],
        "gridPos": {"h": 8, "w": 12, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "Error Rate (%)",
        "type": "graph",
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{status=~\"5..\"}[5m])) by (service) / sum(rate(http_requests_total[5m])) by (service) * 100",
            "legendFormat": "{{service}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {"label": "Error Rate %", "min": 0, "max": 100},
          {"show": false}
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {"params": [5], "type": "gt"},
              "operator": {"type": "and"},
              "query": {"params": ["A", "5m", "now"]},
              "reducer": {"params": [], "type": "avg"},
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "for": "5m",
          "frequency": "10s",
          "handler": 1,
          "name": "High Error Rate Alert",
          "noDataState": "no_data",
          "notifications": []
        },
        "gridPos": {"h": 8, "w": 6, "x": 18, "y": 0}
      },
      {
        "id": 4,
        "title": "Response Time Percentiles",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(http_request_duration_seconds_bucket[5m])) by (le, service))",
            "legendFormat": "{{service}} - 50th",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le, service))",
            "legendFormat": "{{service}} - 95th",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(http_request_duration_seconds_bucket[5m])) by (le, service))",
            "legendFormat": "{{service}} - 99th",
            "refId": "C"
          }
        ],
        "yAxes": [
          {"label": "Response Time (s)", "min": 0},
          {"show": false}
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 5,
        "title": "Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "(jvm_memory_used_bytes / jvm_memory_max_bytes) * 100",
            "legendFormat": "{{service}} - {{area}}",
            "refId": "A"
          }
        ],
        "yAxes": [
          {"label": "Memory Usage %", "min": 0, "max": 100},
          {"show": false}
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 6,
        "title": "Business Metrics",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(user_events_processed_total[5m])",
            "legendFormat": "Events Processed/sec",
            "refId": "A"
          },
          {
            "expr": "rate(struggle_signals_detected_total[5m])",
            "legendFormat": "Struggle Signals/sec",
            "refId": "B"
          },
          {
            "expr": "rate(interventions_executed_total[5m])",
            "legendFormat": "Interventions/sec",
            "refId": "C"
          }
        ],
        "yAxes": [
          {"label": "Events/sec", "min": 0},
          {"show": false}
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16}
      },
      {
        "id": 7,
        "title": "Circuit Breaker Status",
        "type": "table",
        "targets": [
          {
            "expr": "circuit_breaker_state",
            "legendFormat": "",
            "refId": "A",
            "format": "table",
            "instant": true
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {"Time": true, "__name__": true},
              "indexByName": {},
              "renameByName": {
                "service_name": "Service",
                "state": "State",
                "Value": "Status"
              }
            }
          }
        ],
        "fieldConfig": {
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Status"},
              "properties": [
                {
                  "id": "custom.displayMode",
                  "value": "color-background"
                },
                {
                  "id": "thresholds",
                  "value": {
                    "steps": [
                      {"color": "green", "value": 0},
                      {"color": "yellow", "value": 0.5},
                      {"color": "red", "value": 1}
                    ]
                  }
                }
              ]
            }
          ]
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16}
      },
      {
        "id": 8,
        "title": "AWS Services Health",
        "type": "stat",
        "targets": [
          {
            "expr": "aws_dynamodb_successful_request_latency_average",
            "legendFormat": "DynamoDB Latency (ms)",
            "refId": "A"
          },
          {
            "expr": "aws_kinesis_incoming_records_sum",
            "legendFormat": "Kinesis Records/min",
            "refId": "B"
          },
          {
            "expr": "aws_sqs_approximate_number_of_messages",
            "legendFormat": "SQS Queue Depth",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 100},
                {"color": "red", "value": 1000}
              ]
            }
          }
        },
        "gridPos": {"h": 4, "w": 24, "x": 0, "y": 24}
      }
    ],
    "templating": {
      "list": [
        {
          "name": "service",
          "type": "query",
          "query": "label_values(up, service)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        },
        {
          "name": "instance",
          "type": "query",
          "query": "label_values(up{service=\"$service\"}, instance)",
          "refresh": 1,
          "includeAll": true,
          "multi": true
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "Prometheus",
          "expr": "changes(up[1m]) > 0",
          "titleFormat": "Service Restart",
          "textFormat": "{{service}} restarted"
        }
      ]
    }
  }
}