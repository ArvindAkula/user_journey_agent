# User Application Staging Environment Configuration

# Server Configuration
PORT=3000
REACT_APP_API_BASE_URL=https://staging-api.userjourney.com
REACT_APP_WEBSOCKET_URL=wss://staging-api.userjourney.com/ws

# Application Configuration
REACT_APP_ENVIRONMENT=staging
REACT_APP_APP_NAME=User Application (Staging)
REACT_APP_VERSION=1.0.0
REACT_APP_APP_TYPE=user

# User App Specific Configuration
REACT_APP_EVENT_BATCH_SIZE=15
REACT_APP_EVENT_FLUSH_INTERVAL=4000
REACT_APP_OFFLINE_QUEUE_SIZE=150
REACT_APP_ANALYTICS_ENABLED=true
REACT_APP_DEBUG_MODE=true

# Firebase Configuration for User Authentication (Staging)
REACT_APP_FIREBASE_API_KEY=
REACT_APP_FIREBASE_AUTH_DOMAIN=
REACT_APP_FIREBASE_PROJECT_ID=user-journey-app-staging
REACT_APP_FIREBASE_STORAGE_BUCKET=
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=
REACT_APP_FIREBASE_APP_ID=

# Feature Flags
REACT_APP_ENABLE_VIDEO_LIBRARY=true
REACT_APP_ENABLE_CALCULATOR=true
REACT_APP_ENABLE_DOCUMENT_UPLOAD=true
REACT_APP_ENABLE_PERSONA_SWITCHER=true