# Analytics Dashboard Staging Environment Configuration

# Server Configuration
PORT=3001
REACT_APP_API_BASE_URL=https://staging-api.userjourney.com
REACT_APP_ANALYTICS_API_BASE_URL=https://staging-api.userjourney.com/api/analytics
REACT_APP_WEBSOCKET_URL=wss://staging-api.userjourney.com/ws/analytics
REACT_APP_REALTIME_ENDPOINT=wss://staging-api.userjourney.com/ws/realtime

# Application Configuration
REACT_APP_ENVIRONMENT=staging
REACT_APP_APP_NAME=Analytics Dashboard (Staging)
REACT_APP_VERSION=1.0.0
REACT_APP_APP_TYPE=analytics

# Analytics Specific Configuration
REACT_APP_ANALYTICS_REFRESH_INTERVAL=45000
REACT_APP_EXPORT_MAX_RECORDS=25000
REACT_APP_CHART_ANIMATION_DURATION=250
REACT_APP_DATA_RETENTION_DAYS=180
REACT_APP_MAX_CONCURRENT_EXPORTS=4
REACT_APP_REALTIME_BUFFER_SIZE=1500

# Authentication Configuration for Analytics Team
REACT_APP_AUTH_PROVIDER=jwt
REACT_APP_AUTH_ENDPOINT=https://staging-api.userjourney.com/api/auth/analytics
REACT_APP_TOKEN_REFRESH_INTERVAL=3600000
REACT_APP_SESSION_TIMEOUT=7200000

# Feature Flags for Analytics
REACT_APP_ENABLE_AMAZON_Q=true
REACT_APP_ENABLE_EXPORT=true
REACT_APP_ENABLE_REALTIME=true
REACT_APP_ENABLE_ADVANCED_FILTERS=true
REACT_APP_ENABLE_USER_SEGMENTATION=true

# Performance Configuration
REACT_APP_PAGINATION_SIZE=75
REACT_APP_CHART_MAX_DATA_POINTS=1500
REACT_APP_DEBOUNCE_DELAY=250