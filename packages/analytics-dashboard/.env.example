# Analytics Dashboard Environment Configuration

# Server Configuration
PORT=3001
REACT_APP_API_BASE_URL=http://localhost:8080
REACT_APP_ANALYTICS_API_BASE_URL=http://localhost:8080/api/analytics
REACT_APP_WEBSOCKET_URL=ws://localhost:8080/ws/analytics
REACT_APP_REALTIME_ENDPOINT=ws://localhost:8080/ws/realtime

# Application Configuration
REACT_APP_ENVIRONMENT=development
REACT_APP_APP_NAME=Analytics Dashboard
REACT_APP_VERSION=1.0.0
REACT_APP_APP_TYPE=analytics

# Analytics Specific Configuration
REACT_APP_ANALYTICS_REFRESH_INTERVAL=30000
REACT_APP_EXPORT_MAX_RECORDS=10000
REACT_APP_CHART_ANIMATION_DURATION=300
REACT_APP_DATA_RETENTION_DAYS=90
REACT_APP_MAX_CONCURRENT_EXPORTS=3
REACT_APP_REALTIME_BUFFER_SIZE=1000

# Authentication Configuration for Analytics Team
REACT_APP_AUTH_PROVIDER=jwt
REACT_APP_AUTH_ENDPOINT=http://localhost:8080/api/auth/analytics
REACT_APP_TOKEN_REFRESH_INTERVAL=3600000
REACT_APP_SESSION_TIMEOUT=7200000

# CDN Configuration (Production)
# REACT_APP_CDN_URL=https://cdn.yourdomain.com/analytics-dashboard/

# Error Reporting (Production)
# REACT_APP_ERROR_REPORTING_ENDPOINT=https://api.yourdomain.com
# REACT_APP_ERROR_REPORTING_API_KEY=your_error_reporting_api_key
# REACT_APP_SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id

# Performance Monitoring
# REACT_APP_ENABLE_PERFORMANCE_MONITORING=true

# Build Optimizations (Production)
# GENERATE_SOURCEMAP=false
# ANALYZE_BUNDLE=false

# Feature Flags for Analytics
REACT_APP_ENABLE_AMAZON_Q=true
REACT_APP_ENABLE_EXPORT=true
REACT_APP_ENABLE_REALTIME=true
REACT_APP_ENABLE_ADVANCED_FILTERS=true
REACT_APP_ENABLE_USER_SEGMENTATION=true

# Performance Configuration
REACT_APP_PAGINATION_SIZE=50
REACT_APP_CHART_MAX_DATA_POINTS=1000
REACT_APP_DEBOUNCE_DELAY=300