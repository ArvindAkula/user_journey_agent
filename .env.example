# =============================================================================
# AWS Configuration
# =============================================================================
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-aws-access-key
AWS_SECRET_ACCESS_KEY=your-aws-secret-key

# =============================================================================
# AWS Service Names
# =============================================================================

# Kinesis
AWS_KINESIS_STREAM_NAME=user-journey-analytics-user-events

# DynamoDB Tables
AWS_DYNAMODB_USER_EVENTS_TABLE=user-journey-analytics-user-events
AWS_DYNAMODB_STRUGGLE_SIGNALS_TABLE=user-journey-analytics-struggle-signals
AWS_DYNAMODB_USER_PROFILES_TABLE=user-journey-analytics-user-profiles
AWS_DYNAMODB_VIDEO_ENGAGEMENT_TABLE=user-journey-analytics-video-engagement
AWS_DYNAMODB_INTERVENTIONS_TABLE=user-journey-analytics-interventions

# S3
AWS_S3_BUCKET_NAME=user-journey-analytics-data-YOUR_ACCOUNT_ID
AWS_S3_REGION=us-east-1

# SQS Configuration (replace YOUR_ACCOUNT_ID with your AWS account ID)
AWS_SQS_EVENT_PROCESSING_QUEUE_URL=https://sqs.us-east-1.amazonaws.com/YOUR_ACCOUNT_ID/user-journey-analytics-event-processing
AWS_SQS_EVENT_PROCESSING_DLQ_URL=https://sqs.us-east-1.amazonaws.com/YOUR_ACCOUNT_ID/user-journey-analytics-event-processing-dlq
AWS_SQS_STRUGGLE_SIGNALS_QUEUE_URL=https://sqs.us-east-1.amazonaws.com/YOUR_ACCOUNT_ID/user-journey-analytics-struggle-signals
AWS_SQS_VIDEO_ANALYSIS_QUEUE_URL=https://sqs.us-east-1.amazonaws.com/YOUR_ACCOUNT_ID/user-journey-analytics-video-analysis
AWS_SQS_INTERVENTION_EXECUTION_QUEUE_URL=https://sqs.us-east-1.amazonaws.com/YOUR_ACCOUNT_ID/user-journey-analytics-intervention-execution
AWS_SQS_DLQ_URL=https://sqs.us-east-1.amazonaws.com/YOUR_ACCOUNT_ID/user-journey-analytics-event-processing-dlq
AWS_SQS_RETRY_QUEUE_URL=https://sqs.us-east-1.amazonaws.com/YOUR_ACCOUNT_ID/user-journey-analytics-event-processing-dlq

# Bedrock Configuration
AWS_BEDROCK_REGION=us-east-1
AWS_BEDROCK_MODEL_ID=amazon.nova-micro-v1:0
AWS_BEDROCK_AGENT_ID=your-agent-id
AWS_BEDROCK_AGENT_ALIAS_ID=your-agent-alias-id

# SageMaker
AWS_SAGEMAKER_ENDPOINT_NAME=user-journey-analytics-exit-risk-endpoint

# =============================================================================
# Firebase Configuration
# =============================================================================
FIREBASE_PROJECT_ID=your-firebase-project-id
FIREBASE_API_KEY=your-firebase-api-key
FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
FIREBASE_STORAGE_BUCKET=your-project.firebasestorage.app
FIREBASE_MESSAGING_SENDER_ID=your-sender-id
FIREBASE_APP_ID=your-app-id
FIREBASE_MEASUREMENT_ID=your-measurement-id

# =============================================================================
# Application Configuration
# =============================================================================
SERVER_PORT=8080
SPRING_PROFILES_ACTIVE=dev

# CORS
CORS_ALLOWED_ORIGINS=http://localhost:3000,http://localhost:3001

# Logging
LOGGING_LEVEL_ROOT=INFO
LOGGING_LEVEL_COM_USERJOURNEY=DEBUG

# =============================================================================
# Feature Flags
# =============================================================================
FEATURE_BEDROCK_ENABLED=true
FEATURE_SAGEMAKER_ENABLED=true
FEATURE_REAL_TIME_ANALYTICS=true
FEATURE_VIDEO_INTELLIGENCE=true
FEATURE_PREDICTIVE_ANALYTICS=true

# =============================================================================
# Monitoring & Observability
# =============================================================================
CLOUDWATCH_NAMESPACE=UserJourneyAnalytics
CLOUDWATCH_LOG_GROUP=/aws/user-journey-analytics

# =============================================================================
# Security
# =============================================================================
JWT_SECRET=your-jwt-secret-key-change-this
JWT_EXPIRATION=86400000

# Rate Limiting
RATE_LIMIT_REQUESTS_PER_MINUTE=100
RATE_LIMIT_BURST_CAPACITY=200
